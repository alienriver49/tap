{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "lib": ["dom", "es2015"],
        "sourceMap": true,
        "inlineSources": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitReturns": true,
        "preserveConstEnums": true,
        "strictNullChecks": true,
        "pretty": true,
        "types": [
            "jasmine"
        ]
    },
    "exclude": [
        "../node_modules",
        "exts",
        "web-components"
    ]
}
